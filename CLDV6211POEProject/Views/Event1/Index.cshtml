            <th>
                @Html.DisplayNameFor(model => model.Venue_Id)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Event_Id)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.Event_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Event_Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Venue_Id)
            </td>
           <td>
               <a asp-action="Edit" asp-route-id="@item.Event_Id">Edit</a>
